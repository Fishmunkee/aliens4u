<div class="container-index">
  <div class= "cards">
    <% @aliens.each do |alien| %>
      <div class="card">
        <h3><%= link_to alien.name, alien_path(alien.id) %></h3>
        <h3><%= alien.species %></h3>
        <h3><%= alien.planet %></h3>
      </div>
    <% end %>
  </div>
</div>
<%= link_to "Create a new Alien?", new_alien_path %>

