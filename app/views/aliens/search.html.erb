<div class="container-index">
  <div class= "cards">
    <% @aliens.each do |alien| %>
      <div class="card">
        <h3>Name: <%= link_to alien.name, alien_path(alien.id) %></h3>
        <h3>Species: <%= alien.species %></h3>
        <h3>Planet of Origin: <%= alien.planet %></h3>
      </div>
    <% end %>
  </div>
</div>
